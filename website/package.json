{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "fmt": "prettier --write . src/**/*.astro",
    "fmt:check": "prettier --check . src/**/*.astro",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^6.6.1",
    "@astrojs/react": "^2.2.1",
    "@astrojs/tailwind": "^4.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "astro": "^2.8.3",
    "class-variance-authority": "^0.6.1",
    "copy-text-to-clipboard": "^3.2.0",
    "date-fns": "^2.30.0",
    "fuse.js": "^6.6.2",
    "js-yaml": "^4.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@radix-ui/colors": "^1.0.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "3.0.0",
    "prettier-plugin-astro": "^0.11.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-pkg": "^0.18.0",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-pkg",
      "prettier-plugin-organize-imports"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
